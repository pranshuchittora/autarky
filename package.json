{
  "name": "autarky",
  "version": "1.2.4",
  "description": "Liberating disk space from node_modules",
  "author": "Pranshu Chittora <pranshuchittora17@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/pranshuchittora/autarky#readme",
  "repository": {
    "type": "git",
    "url": "git://github.com/pranshuchittora/autarky.git"
  },
  "bugs": {
    "url": "https://github.com/pranshuchittora/autarky/issues"
  },
  "bin": {
    "autarky": "build/index.js"
  },
  "main": "build/index",
  "types": "build/index.d.ts",
  "dependencies": {
    "chalk": "^3.0.0",
    "fs": "^0.0.1-security",
    "g-factor": "^1.1.0",
    "inquirer": "^7.0.0",
    "moment": "^2.24.0",
    "redux": "^4.0.4",
    "rimraf": "^3.0.0"
  },
  "scripts": {
    "watch": "tsc -p tsconfig.dev.json --watch",
    "build": "yarn run format &&  tsc -p tsconfig.prod.json",
    "format": "prettier --write 'src/**/*.*'",
    "test": "jest",
    "test-watch": "jest --watchAll",
    "prepublish": "yarn run build"
  },
  "files": [
    "build/**/*"
  ],
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^24.0.23",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.0.14",
    "@types/redux": "^3.6.0",
    "@types/rimraf": "^2.0.3",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.2"
  },
  "keywords": [
    "cli",
    "typescript",
    "nodejs",
    "node_modules",
    "storage manager",
    "disk space"
  ]
}
